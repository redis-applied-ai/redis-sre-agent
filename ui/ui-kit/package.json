{
  "name": "@radar/ui-kit",
  "version": "0.1.0",
  "private": false,
  "description": "Shared UI component library with Redis-themed design system",
  "keywords": [
    "react",
    "typescript",
    "ui",
    "components",
    "tailwindcss",
    "design-system",
    "redis"
  ],
  "homepage": "https://github.com/your-org/radar-ui-kit#readme",
  "bugs": {
    "url": "https://github.com/your-org/radar-ui-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/radar-ui-kit.git"
  },
  "license": "MIT",
  "author": "Radar Team",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist",
    "dev": "vite build --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest dev",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@storybook/addon-essentials": "^8.5.6",
    "@storybook/addon-interactions": "^8.5.6",
    "@storybook/addon-links": "^8.5.6",
    "@storybook/blocks": "^8.5.6",
    "@storybook/react": "^8.5.6",
    "@storybook/react-vite": "^8.5.6",
    "@storybook/test": "^8.5.6",
    "@tailwindcss/postcss": "^4.1.6",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sort-package-json": "^3.4.0",
    "storybook": "^8.5.6",
    "tailwindcss": "^4.1.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
