{
  "name": "redis-sre",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "e2e": "playwright test",
    "e2e:clean": "node scripts/cleanup-e2e.mjs",
    "e2e:headed": "playwright test --headed --project=chromium",
    "e2e:ui": "playwright test --ui",
    "format": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "postinstall": "npm --prefix ./ui-kit ci && npm --prefix ./ui-kit run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radar/ui-kit": "file:./ui-kit",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.6",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.6",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
